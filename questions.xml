<quiz>
    <round title="Round 1: General Science" time="900" maxScore="20">
        <question>
            <text>Which gas is most abundant in Earth's atmosphere?</text>
            <options>
                <option>Oxygen</option>
                <option>Carbon Dioxide</option>
